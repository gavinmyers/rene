<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:cc="clr-namespace:Rene.CustomControl;assembly=Rene"
    >


	<Style x:Key="ArrowRight" TargetType="{x:Type cc:Icon}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type cc:Icon}">
					<Grid Width="15" Height="15">
							<Path Width="5.18056" Height="5" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Fill" Fill="#FF000000" Data="F1 M 0,0L 0,5L 5.18056,2.5L 0,0 Z "/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="ArrowLeft" TargetType="{x:Type cc:Icon}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type cc:Icon}">
					<Grid Width="15" Height="15">
						<Path x:Name="Path" Width="5.18056" Height="5" Canvas.Left="3.67114e-014" Canvas.Top="0" Stretch="Fill" Fill="#FF000000" Data="F1 M 5.18056,5L 5.18056,0L 3.67114e-014,2.5L 5.18056,5 Z "/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	<Style x:Key="Calendar" TargetType="{x:Type cc:Icon}">
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type cc:Icon}">
					<Canvas Width="15" Height="15" Margin="0,0,10,0" Canvas.Left="5" Canvas.Top="3">
						<Rectangle x:Name="Rectangle" Width="16" Height="14.983" Canvas.Left="-0.5" Canvas.Top="0.517029" Stretch="Fill" StrokeLineJoin="Round" Stroke="#FF838383">
							<Rectangle.Fill>
								<LinearGradientBrush StartPoint="0.568701,0.437871" EndPoint="0.568701,1.01291">
									<LinearGradientBrush.GradientStops>
										<GradientStop Color="#FFFFFFFF" Offset="0"/>
										<GradientStop Color="#FFBABABA" Offset="1"/>
									</LinearGradientBrush.GradientStops>
								</LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
						<Rectangle x:Name="Rectangle_0" Width="16" Height="6.08472" Canvas.Left="-0.5" Canvas.Top="0.517029" Stretch="Fill" StrokeLineJoin="Round" Stroke="#FFA6A6A6">
							<Rectangle.Fill>
								<LinearGradientBrush StartPoint="0.495384,0.346878" EndPoint="0.495384,2.89875">
									<LinearGradientBrush.GradientStops>
										<GradientStop Color="#FFFF5000" Offset="0"/>
										<GradientStop Color="#FF250B00" Offset="1"/>
									</LinearGradientBrush.GradientStops>
								</LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
						<Path x:Name="Path" Width="4" Height="4.81354" Canvas.Left="10" Canvas.Top="-0.499939" Stretch="Fill" StrokeLineJoin="Round" Stroke="#FF989898" Data="F1 M 12,6.10352e-005C 12.8284,6.10352e-005 13.5,0.85376 13.5,1.9068C 13.5,2.9599 12.8284,3.8136 12,3.8136C 11.1716,3.8136 10.5,2.9599 10.5,1.9068C 10.5,0.85376 11.1716,6.10352e-005 12,6.10352e-005 Z ">
							<Path.Fill>
								<RadialGradientBrush RadiusX="0.5625" RadiusY="0.39612" Center="0.5,0.499994" GradientOrigin="0.5,0.499994">
									<RadialGradientBrush.GradientStops>
										<GradientStop Color="#FFFFFFFF" Offset="0.4"/>
										<GradientStop Color="#FF838383" Offset="1"/>
									</RadialGradientBrush.GradientStops>
									<RadialGradientBrush.RelativeTransform>
										<TransformGroup/>
									</RadialGradientBrush.RelativeTransform>
								</RadialGradientBrush>
							</Path.Fill>
						</Path>
						<Path x:Name="Path_1" Width="4" Height="4.81354" Canvas.Left="1" Canvas.Top="-0.499939" Stretch="Fill" StrokeLineJoin="Round" Stroke="#FF989898" Data="F1 M 3,6.10352e-005C 3.82842,6.10352e-005 4.5,0.85376 4.5,1.9068C 4.5,2.9599 3.82843,3.8136 3,3.8136C 2.17157,3.8136 1.5,2.9599 1.5,1.9068C 1.5,0.85376 2.17158,6.10352e-005 3,6.10352e-005 Z ">
							<Path.Fill>
								<RadialGradientBrush RadiusX="0.5625" RadiusY="0.39612" Center="0.5,0.499994" GradientOrigin="0.5,0.499994">
									<RadialGradientBrush.GradientStops>
										<GradientStop Color="#FFFFFFFF" Offset="0.4"/>
										<GradientStop Color="#FF838383" Offset="1"/>
									</RadialGradientBrush.GradientStops>
									<RadialGradientBrush.RelativeTransform>
										<TransformGroup/>
									</RadialGradientBrush.RelativeTransform>
								</RadialGradientBrush>
							</Path.Fill>
						</Path>
						<TextBlock Text="17" Canvas.Left="3" Canvas.Top="6" FontSize="8" Foreground="#FF0B0B0B" x:Name="textBlock"/>
						<Path x:Name="Path_3" Width="6" Height="4.75" Canvas.Left="9.5" Canvas.Top="10.75" Stretch="Fill" StrokeLineJoin="Round" Stroke="#FF676767" Fill="#FFCFCFCF" Data="F1 M 10,15L 15,11.25L 10,11.25L 10,15 Z "/>
					</Canvas>
					<ControlTemplate.Triggers>
						<Trigger Property="IsMouseOver" Value="True">
							<Setter Property="Stroke" TargetName="Rectangle" Value="#FFCBCBCB"/>
							<Setter Property="Stroke" TargetName="Rectangle_0" Value="#FFD9D9D9"/>
							<Setter Property="Stroke" TargetName="Path" Value="#FF8E8E8E"/>
							<Setter Property="Stroke" TargetName="Path_1" Value="#FF818181"/>
							<Setter Property="Fill" TargetName="Rectangle_0">
								<Setter.Value>
									<LinearGradientBrush EndPoint="0.495384,2.89875" StartPoint="0.495384,0.346878">
										<GradientStop Color="#FFE7A282" Offset="0"/>
										<GradientStop Color="#FF250B00" Offset="1"/>
									</LinearGradientBrush>
								</Setter.Value>
							</Setter>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	
	
</ResourceDictionary>