<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >


	<SolidColorBrush x:Key="CheckBoxStroke" Color="#8E8F8F"/>




	<Style TargetType="{x:Type CheckBox}">
		<Setter Property="SnapsToDevicePixels" Value="true"/>
		<Setter Property="FocusVisualStyle" Value="{x:Null}"/>
		<Setter Property="Background" Value="{DynamicResource NormalBrush}"/>
		<Setter Property="Margin" Value="0,0,0,5"/>
		<Setter Property="BorderBrush" Value="{DynamicResource NormalBorderBrush}"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type CheckBox}">
					<DockPanel
						Margin="0" 
						HorizontalAlignment="Stretch" 
						VerticalAlignment="Top" 
						Width="Auto" 
						LastChildFill="True">



						<Border x:Name="MouseUpUnchecked" Width="12" Height="12" BorderThickness="1,1,1,1">
							<Border.BorderBrush>
								<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476189,0.0276358">
									<LinearGradientBrush.GradientStops>
										<GradientStop Color="#FF7A7A7A" Offset="0"/>
										<GradientStop Color="#FFC4C4C4" Offset="1"/>
									</LinearGradientBrush.GradientStops>
								</LinearGradientBrush>
							</Border.BorderBrush>
							<Border.Background>
								<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476189,0.0276358">
									<LinearGradientBrush.GradientStops>
										<GradientStop Color="#FFFFFFFF" Offset="0"/>
										<GradientStop Color="#FFE5E6E7" Offset="1"/>
									</LinearGradientBrush.GradientStops>
								</LinearGradientBrush>
							</Border.Background>
							<Border BorderBrush="#FFFFFFFF" BorderThickness="1,1,1,1">

							</Border>
						</Border>



						<Border x:Name="MouseOverUnchecked" Width="12" Height="12" BorderBrush="#FFB5B5B5" BorderThickness="1,1,1,1">
							<Border.Background>
								<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476183,0.0276358">
									<GradientStop Color="#FFFFFFFF" Offset="0"/>
									<GradientStop Color="#FFD9D9D9" Offset="0.506849"/>
									<GradientStop Color="#FFA2A2A2" Offset="1"/>
								</LinearGradientBrush>
							</Border.Background>
							<Border BorderBrush="#86FFFFFF" BorderThickness="1,1,1,1">
								<Border Margin="0.5,0.5,0.5,0" VerticalAlignment="Top" Background="#4DFFFFFF" Width="Auto" Height="4"/>
							</Border>
						</Border>


						<Grid x:Name="MouseOverChecked">
							<Border Width="12" Height="12" BorderBrush="#FFCBCBCB" BorderThickness="1,1,1,1">
								<Border.Background>
									<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476183,0.0276358">
										<GradientStop Color="#FFFCFCFC" Offset="0"/>
										<GradientStop Color="#FFD9D9D9" Offset="0.506849"/>
										<GradientStop Color="#FFD9D9D9" Offset="1"/>
									</LinearGradientBrush>
								</Border.Background>
								<Border BorderBrush="#86FFFFFF" BorderThickness="1,1,1,1">
									<Border Margin="0.5,0.5,0.5,0" VerticalAlignment="Top" Background="#4DFFFFFF" Width="Auto" Height="4">
									</Border>
								</Border>
							</Border>
							<Path Width="8.65974" Height="9.34027" Canvas.Left="6.06175" Canvas.Top="3.66327" Stretch="Fill" Fill="#FFC5C5C5" Data="F1 M 6.06175,9.2605L 7.68329,7.75354L 9.82217,10.2813L 13.3777,3.67017L 14.6937,3.66327L 14.7215,5.49658L 11.1625,13.0035L 9.20413,12.9619L 6.06175,9.2605 Z " Margin="0,0,-1,0"/>
							<Path Width="8.65976" Height="9.34033" Canvas.Left="5.96454" Canvas.Top="2.40967" Stretch="Fill" Fill="#FF949494" Data="F1 M 5.96454,8.00702L 7.58609,6.5L 9.72496,9.02777L 13.2805,2.41663L 14.5965,2.40967L 14.6243,4.24304L 11.0652,11.75L 9.10692,11.7084L 5.96454,8.00702 Z " Margin="0,-2,-1,0"/>
						</Grid>



						<Border x:Name="MouseDownUnchecked" Width="12" Height="12" BorderBrush="#FFD3D3D3" BorderThickness="1,1,1,1">
							<Border.Background>
								<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476183,0.0276358">
									<GradientStop Color="#FFFFFFFF" Offset="0"/>
									<GradientStop Color="#FFD3D3D3" Offset="0.506849"/>
									<GradientStop Color="#FF8C8C8C" Offset="1"/>
								</LinearGradientBrush>
							</Border.Background>
							<Border BorderThickness="1,1,1,1">
								<Border Margin="0.5,0.5,0.5,0" VerticalAlignment="Top" Background="#4DFFFFFF" Width="Auto" Height="4"/>
							</Border>
						</Border>

						<Grid x:Name="MouseDownChecked">
							<Border Width="12" Height="12" BorderBrush="#FFD9D9D9" BorderThickness="1,1,1,1">
								<Border.Background>
									<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476183,0.0276358">
										<GradientStop Color="#FFA2A2A2" Offset="0"/>
										<GradientStop Color="#FFD3D3D3" Offset="0.506849"/>
										<GradientStop Color="#FFE1E1E1" Offset="1"/>
									</LinearGradientBrush>
								</Border.Background>
								<Border BorderThickness="1,1,1,1">
									<Border Margin="0.5,0.5,0.5,0" VerticalAlignment="Top" Background="#4DFFFFFF" Width="Auto" Height="4">
									</Border>
								</Border>
							</Border>
							<Path Width="8.65974" Height="9.34027" Canvas.Left="6.06175" Canvas.Top="3.66327" Stretch="Fill" Fill="#FFDFDFDF" Data="F1 M 6.06175,9.2605L 7.68329,7.75354L 9.82217,10.2813L 13.3777,3.67017L 14.6937,3.66327L 14.7215,5.49658L 11.1625,13.0035L 9.20413,12.9619L 6.06175,9.2605 Z " Margin="0,0,-1,0"/>
							<Path Width="8.65976" Height="9.34033" Canvas.Left="5.96454" Canvas.Top="2.40967" Stretch="Fill" Fill="#FF797979" Data="F1 M 5.96454,8.00702L 7.58609,6.5L 9.72496,9.02777L 13.2805,2.41663L 14.5965,2.40967L 14.6243,4.24304L 11.0652,11.75L 9.10692,11.7084L 5.96454,8.00702 Z " Margin="0,-2,-1,0"/>
						</Grid>

						<Grid x:Name="MouseUpChecked">
							<Border Width="12" Height="12" BorderThickness="1,1,1,1">
								<Border.BorderBrush>
									<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476189,0.0276358">
										<LinearGradientBrush.GradientStops>
											<GradientStop Color="#FF7A7A7A" Offset="0"/>
											<GradientStop Color="#FFC4C4C4" Offset="1"/>
										</LinearGradientBrush.GradientStops>
									</LinearGradientBrush>
								</Border.BorderBrush>
								<Border.Background>
									<LinearGradientBrush StartPoint="0.97619,0.956207" EndPoint="0.0476189,0.0276358">
										<LinearGradientBrush.GradientStops>
											<GradientStop Color="#FFFFFFFF" Offset="0"/>
											<GradientStop Color="#FFE5E6E7" Offset="1"/>
										</LinearGradientBrush.GradientStops>
									</LinearGradientBrush>
								</Border.Background>
								<Border BorderBrush="#FFFFFFFF" BorderThickness="1,1,1,1">

								</Border>
							</Border>
							<Path Width="8.65974" Height="9.34027" Canvas.Left="6.06175" Canvas.Top="3.66327" Stretch="Fill" Fill="#FFDCDCDC" Data="F1 M 6.06175,9.2605L 7.68329,7.75354L 9.82217,10.2813L 13.3777,3.67017L 14.6937,3.66327L 14.7215,5.49658L 11.1625,13.0035L 9.20413,12.9619L 6.06175,9.2605 Z " Margin="0,0,-1,0"/>
							<Path Width="8.65976" Height="9.34033" Canvas.Left="5.96454" Canvas.Top="2.40967" Stretch="Fill" Fill="#FF303030" Data="F1 M 5.96454,8.00702L 7.58609,6.5L 9.72496,9.02777L 13.2805,2.41663L 14.5965,2.40967L 14.6243,4.24304L 11.0652,11.75L 9.10692,11.7084L 5.96454,8.00702 Z " Margin="0,-2,-1,0"/>

						</Grid>

						<Border Width="10" Background="#02FFFFFF"/>

						<ContentPresenter 
							Content="{TemplateBinding Content}" 
							RecognizesAccessKey="True" 
							Height="Auto" 
							HorizontalAlignment="Left" VerticalAlignment="Center" />

					</DockPanel>
					<ControlTemplate.Triggers>

						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="True"/>
								<Condition Property="IsChecked" Value="False"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Visible"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="True"/>
								<Condition Property="IsChecked" Value="True"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False"/>
								<Condition Property="IsChecked" Value="True"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False"/>
								<Condition Property="IsChecked" Value="False"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
						</MultiTrigger>

						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsChecked" Value="True"/>
								<Condition Property="IsPressed" Value="True"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsChecked" Value="False"/>
								<Condition Property="IsPressed" Value="True"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
						</MultiTrigger>

						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False"/>
								<Condition Property="IsChecked" Value="True"/>
								<Condition Property="IsEnabled" Value="False"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
							<Setter Property="Opacity" TargetName="MouseUpChecked" Value="0.5"/>
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="IsMouseOver" Value="False"/>
								<Condition Property="IsChecked" Value="False"/>
								<Condition Property="IsEnabled" Value="False"/>
							</MultiTrigger.Conditions>
							<Setter Property="Visibility" TargetName="MouseUpUnchecked" Value="Visible"/>
							<Setter Property="Visibility" TargetName="MouseOverChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownUnchecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseDownChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseUpChecked" Value="Collapsed"/>
							<Setter Property="Visibility" TargetName="MouseOverUnchecked" Value="Collapsed"/>
							<Setter Property="Opacity" TargetName="MouseUpUnchecked" Value="0.5"/>
						</MultiTrigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>