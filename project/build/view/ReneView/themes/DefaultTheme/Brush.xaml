				<Path Width="6" Height="6" Canvas.Left="-0.5" Canvas.Top="-0.5" Stretch="Fill" StrokeLineJoin="Round" Stroke="#0D000000" Data="F1 M 0,5L 5,0"/>
			</Canvas>

		</VisualBrush.Visual>
	</VisualBrush>


	<SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#8CAFAFAF" />
	<SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="#F2000000" />

	<BorderGapMaskConverter x:Key="BorderGapMaskConverter"/>
	


</ResourceDictionary>