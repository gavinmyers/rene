				<Path Width="21" Height="21" Canvas.Left="29.5" Canvas.Top="29.5" Stretch="Fill" StrokeLineJoin="Round" Stroke="#27000000" Data="F1 M 30,50L 50,30"/>
				<Path Width="16" Height="16" Canvas.Left="34.5" Canvas.Top="34.5" Stretch="Fill" StrokeLineJoin="Round" Stroke="#27000000" Data="F1 M 35,50L 50,35"/>
				<Path Width="11" Height="11" Canvas.Left="39.5" Canvas.Top="39.5" Stretch="Fill" StrokeLineJoin="Round" Stroke="#27000000" Data="F1 M 40,50L 50,40"/>
				<Path Width="6" Height="6" Canvas.Left="44.5" Canvas.Top="44.5" Stretch="Fill" StrokeLineJoin="Round" Stroke="#27000000" Data="F1 M 45,50L 50,45"/>
