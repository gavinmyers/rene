<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:gm="clr-namespace:ReneGameLogic.Sprite;assembly=ReneGameLogic"
	xmlns:cc="clr-namespace:Rene.CustomControl;assembly=Rene"
	xmlns:ccr="clr-namespace:ReneConsumer.CustomControl;assembly=ReneConsumer"
    xmlns:ccv="clr-namespace:ReneConsumer.View;assembly=ReneConsumer"
	xmlns:do="clr-namespace:ReneData.DataObject;assembly=ReneConsumer"
	xmlns:m="clr-namespace:ReneConsumer.Model;assembly=ReneConsumer"
    >

	<Style TargetType="{x:Type ccv:DashboardWindowView}">
		<Setter Property="Width" Value="640"/>
		<Setter Property="Height" Value="480"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ccv:DashboardWindowView}">
					<Grid Background="#DEDEDE" x:Name="PART_Drag">
						<Grid x:Name="PART_Background"/>
						<Grid x:Name="PART_Map"/>
						<Grid x:Name="PART_Tanks" />
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

</ResourceDictionary>